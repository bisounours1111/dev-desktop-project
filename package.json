{
  "name": "human-agement",
  "private": true,
  "version": "1.0.0",
  "description": "Application de gestion de clubs de strip-tease",
  "type": "module",
  "main": "src/main.cjs",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "react-start": "vite",
    "build": "vite build && electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "clean": "rm -rf node_modules/.cache && rm -rf build && rm -rf dist",
    "dev-clean": "npm run clean && npm run dev",
    "postinstall": "electron-builder install-app-deps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org lille-ynov-campus --project electron ./dist && sentry-cli sourcemaps upload --org lille-ynov-campus --project electron ./dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron .",
    "dev:all": "concurrently \"npm run dev\" \"npm run electron\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@mui/system": "^5.17.1",
    "@mui/x-date-pickers": "^6.19.4",
    "@mui/x-date-pickers-pro": "^8.3.0",
    "@sentry/cli": "^2.46.0",
    "@sentry/electron": "^4.24.0",
    "@sentry/node": "^9.26.0",
    "@sentry/react": "^9.26.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.5.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.2.0",
    "firebase": "^10.8.0",
    "hoist-non-react-statics": "^3.0.0-rc.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "react-webcam": "^7.2.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.2.3",
    "electron-builder": "^24.9.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.human-agement.app",
    "productName": "Human'Agement",
    "mac": {
      "category": "public.app-category.business",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "files": [
      "dist/**/*",
      "public/icon.png",
      "src/main.cjs"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "extraResources": [
      {
        "from": "build",
        "to": "build"
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
